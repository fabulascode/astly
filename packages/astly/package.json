    "hastscript": "^5.1.0",
    "prop-types": "^15.7.2",
    "rehype-minify-whitespace": "^3.0.0",
    "rehype-parse": "^6.0.1",
    "rehype-preset-minify": "^4.0.0",
    "rehype-react": "^4.0.1",
    "styled-components": "^4.4.0",
