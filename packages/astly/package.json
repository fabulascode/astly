    "dev": "webpack --watch --mode development --config ./webpack.config.js --env.env=dev",
    "prebuild": "npm run build:clean",
    "postbuild": "./scripts/postbuild/postbuild.sh"
  },
  "devDependencies": {
    "dotenv-webpack": "^1.7.0"
  }
}
